{
  "name": "vapr-makr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "jss": "latest",
    "jss-preset-default": "latest",
    "material-ui": "1.0.0-beta.17",
    "material-ui-icons": "1.0.0-beta.17",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-jss": "latest",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "react-tap-event-plugin": "^3.0.2",
    "recompose": "latest",
    "typeface-roboto": "0.0.41"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/styles/ -o src/assets/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/assets/styles --include-path ./node_modules src/assets/styles/ -o src/assets/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build"
  }
}
